{
  "name": "spikes-api",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && tsc",
    "start": "node ./dist/src/app.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/app.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:generate": "npm run build && npm run typeorm -- migration:generate ./src/migrations/AutoGenerated -d ./dist/app-data-source.js ",
    "migration:run": "npm run build && npm run typeorm -- migration:run -d ./dist/app-data-source.js",
    "migration:revert": "npm run build && npm run typeorm -- migration:revert -d ./dist/app-data-source.js"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.1.0",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "glob": "^11.0.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.0",
    "path": "^0.12.7",
    "resend": "^4.3.0",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "uuid": "^11.1.0",
    "uuidv4": "^6.2.13"
  }
}
